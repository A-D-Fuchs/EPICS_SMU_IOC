InTerminator = '\n';
OutTerminator = '\r\n';
LockTimeout = 5000;
ReplyTimeout = 200;
ReadTimeout = 100;

#Measure voltage DC
mesDCV {
	out "MEAS:VOLT:DC?";
	in "%f";
    ExtraInput = Ignore;
}

#Measure current DC
mesDCI {
	out "MEAS:CURR:DC?";
	in "%f";
    ExtraInput = Ignore;
}

#Measure voltage AC
mesACV {
	out "MEAS:VOLT:AC?";
	in "%f";
    ExtraInput = Ignore;
}

#Measure current AC
mesACI {
	out "MEAS:CURR:AC?";
	in "%f";
    ExtraInput = Ignore;
}

#Measure Resistance

mesR {
	out "MEAS:RES?";
	in "%f";
    ExtraInput = Ignore;
}



#get Identification

idn {
	out "*IDN?";
	in "%1000c";
    ExtraInput = Ignore;
}

wonly {
	out "%s";
}

ronly {
	in "%1000c";
}